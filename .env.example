# conclave Configuration
# Copy this file to .env and configure your settings

# Server Configuration
SERVER_PORT=8182

# AI Provider Configuration
# Enable/disable providers (true/false)
PROVIDER_CLAUDE_ENABLED=true
PROVIDER_GEMINI_ENABLED=true
PROVIDER_QWEN_ENABLED=true
PROVIDER_CODEX_ENABLED=true

# Default Settings
DEFAULT_STYLE=collaborative
DEFAULT_PROVIDER=claude

# Database
DB_PATH=~/.conclave/conclave.db

# Timeout Settings (in seconds)
PROVIDER_TIMEOUT=120
